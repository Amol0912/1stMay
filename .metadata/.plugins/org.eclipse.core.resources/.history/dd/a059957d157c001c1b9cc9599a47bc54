package agsva;

public class A2 {

	public static void main(String[] args) {
		
		String s1 = "Amol";
		String s2 = "Ashok";
		String s3 = "patil";
		String s4 = "pati";
		System.out.println(s1.concat("     "+s2).concat("     "+s3));
		System.out.println(s1.charAt(0));
		System.out.println(s1.codePointAt(4));
	}

}
